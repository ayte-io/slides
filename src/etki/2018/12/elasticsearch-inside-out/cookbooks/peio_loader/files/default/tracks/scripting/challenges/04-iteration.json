{
  "name": "iteration",
  "schedule": [
    {
      "name": "search",
      "time-period": 300,
      "clients": 32,
      "operation": {
        "operation-type": "search",
        "body": {
          "query": {
            "match_all": {}
          },
          "size": 100,
          "track_scores": true,
          "sort": {
            "_script": {
              "type": "number",
              "script": {
                "lang": "painless",
                "source": "int j = 1; for (int i = 1; i < 1000; i++) { j = j * i } return j"
              }
            }
          }
        }
      }
    }
  ]
}
