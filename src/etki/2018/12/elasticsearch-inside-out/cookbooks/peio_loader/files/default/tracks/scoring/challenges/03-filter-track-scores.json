{
  "name": "filter-track-scores",
  "schedule": [
    {
      "name": "search",
      "time-period": 300,
      "clients": 32,
      "operation": {
        "operation-type": "search",
        "body": {
          "query": {
            "bool": {
              "filter": [
                {
                  "match": {
                    "text": "king"
                  }
                }
              ]
            }
          },
          "track_scores": true,
          "size": 100
        }
      }
    }
  ]
}
